				REQUIRED="true" />
			<IMPORT NAME="apache-camel" MODULE="camel-core-2.9.2.jar"
				REQUIRED="true" />
			<IMPORT NAME="spring-core" MODULE="spring-core-3.0.6.RELEASE.jar"
				REQUIRED="true" />
			<IMPORT NAME="talend-camel-ftp" MODULE="camel-ftp-alldep-2.9.2.jar"
				REQUIRED="true" />
			<IMPORT NAME="spring-aop" MODULE="spring-aop-3.0.6.RELEASE.jar"
				REQUIRED="true" />
			<IMPORT NAME="spring-beans" MODULE="spring-beans-3.0.6.RELEASE.jar"
				REQUIRED="true" />
			<IMPORT NAME="spring-tx" MODULE="spring-tx-3.0.6.RELEASE.jar"
				REQUIRED="true" />
			<IMPORT NAME="spring-jms" MODULE="spring-jms-3.0.6.RELEASE.jar"
				REQUIRED="true" />
			<IMPORT NAME="spring-context" MODULE="spring-context-3.0.6.RELEASE.jar"
				REQUIRED="true" />
			<IMPORT NAME="commons-management" MODULE="commons-management-1.0.jar"
				REQUIRED="true" />
			<IMPORT NAME="commons-logging" MODULE="commons-logging-1.1.1.jar"
				REQUIRED="true" />
			<IMPORT NAME="log4j" MODULE="log4j-1.2.16.jar" REQUIRED="true" />
			<IMPORT NAME="slf4j" MODULE="slf4j-api-1.6.1.jar" REQUIRED="true" />
			<IMPORT NAME="slf4j-log4j" MODULE="slf4j-log4j12-1.6.1.jar"
				REQUIRED="true" />
			<IMPORT NAME="activemq" MODULE="activemq-all-5.5.1.jar"
				REQUIRED="true" />
			<IMPORT NAME="activemq-pool" MODULE="activemq-pool-5.5.1.jar"
				REQUIRED="true" />
			<IMPORT NAME="jms" MODULE="camel-jms-alldep-2.9.2.jar"
				REQUIRED="true" />
			<IMPORT NAME="common-pool" MODULE="commons-pool-1.5.4.jar"
				REQUIRED="true" />
