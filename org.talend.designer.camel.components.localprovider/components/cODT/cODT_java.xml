<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<COMPONENT>

	<HEADER AUTHOR="Talend" COMPATIBILITY="ALL" PLATEFORM="ALL"
		RELEASE_DATE="20130620A" SERIAL="" STARTABLE="false" STATUS="ALPHA"
		VERSION="0.1">
		<SIGNATURE />
	</HEADER>

	<FAMILIES>
		<FAMILY>Processor</FAMILY>
	</FAMILIES>

	<DOCUMENTATION>
		<URL />
	</DOCUMENTATION>

	<CONNECTORS>
		<CONNECTOR CTYPE="ROUTE" MAX_INPUT="1" MAX_OUTPUT="1"
			MIN_INPUT="1" MIN_OUTPUT="0" />
		<CONNECTOR CTYPE="ROUTE_WHEN" MAX_INPUT="1" MAX_OUTPUT="0" />
		<CONNECTOR CTYPE="ROUTE_OTHER" MAX_INPUT="1" MAX_OUTPUT="0" />
		<CONNECTOR CTYPE="ROUTE_TRY" MAX_INPUT="1" MAX_OUTPUT="0" />
		<CONNECTOR CTYPE="ROUTE_CATCH" MAX_INPUT="1" MAX_OUTPUT="0" />
		<CONNECTOR CTYPE="ROUTE_FINALLY" MAX_INPUT="1" MAX_OUTPUT="0" />
		<CONNECTOR CTYPE="ROUTE_ENDBLOCK" MAX_INPUT="1" MAX_OUTPUT="0" />
	</CONNECTORS>

	<PARAMETERS>
		
		<PARAMETER NAME="PROJECT_ARCHIVES_TYPE" FIELD="CLOSED_LIST" REQUIRED="false" NUM_ROW="1" GROUP="PROJECT_ARCHIVES">
			<ITEMS DEFAULT="URL">
				<ITEM NAME="URL"		VALUE="url"/>
				<ITEM NAME="REPOSITORY"		VALUE="repository"/>
				<ITEM NAME="FILE"	VALUE="file"/>
			</ITEMS>
		</PARAMETER>

		<PARAMETER NAME="PROJECT_ARCHIVES_LOCATION" FIELD="TEXT" NUM_ROW="4"
		SHOW_IF="PROJECT_ARCHIVES_TYPE == 'URL'" GROUP="PROJECT_ARCHIVES">
			<DEFAULT>"classpath:Examples.zip"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="PROJECT_ARCHIVES_REPO" FIELD="ROUTE_RESOURCE_TYPE" REPOSITORY_VALUE="ROUTER_RESOURCES"   NUM_ROW="6"
			REQUIRED="false" SHOW_IF="PROJECT_ARCHIVES_TYPE == 'REPOSITORY'" GROUP="PROJECT_ARCHIVES">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="PROJECT_ARCHIVES_FILE" FIELD="FILE" NUM_ROW="6"
			REQUIRED="false" GROUP="PROJECT_ARCHIVES"
			SHOW_IF="PROJECT_ARCHIVES_TYPE == 'FILE'">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="ODT_TRANSFORM_PATH" FIELD="TEXT" NUM_ROW="10" REQUIRED="false">
			<DEFAULT>"Examples/Maps/PayPal/POPayPal_PO2"</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="LOG_CONFIG" FIELD="CLOSED_LIST" REQUIRED="false" NUM_ROW="20">
			<ITEMS DEFAULT="DEFAULT">
				<ITEM NAME="DEFAULT"				VALUE=''/>
				<ITEM NAME="ALL"					VALUE='"all"'/>
				<ITEM NAME="ALLBUTVALIDATE"			VALUE='"allButValidate"'/>
				<ITEM NAME="BASENODEDETAIL"			VALUE='"baseNodeDetail"'/>
				<ITEM NAME="COMMANDLINE"			VALUE='"commandLine"'/>
				<ITEM NAME="DATABASE"				VALUE='"database"'/>
				<ITEM NAME="DOCLOCATION"			VALUE='"docLocation"'/>
				<ITEM NAME="DOCUMENTLOCATORREADER"	VALUE='"documentLocatorReader"'/>
				<ITEM NAME="FLAT"					VALUE='"flat"'/>
				<ITEM NAME="FREQUENT"				VALUE='"frequent"'/>
				<ITEM NAME="IMPORTEXPORTDETAIL"		VALUE='"importExportDetail"'/>
				<ITEM NAME="INFO"					VALUE='"info"'/>
				<ITEM NAME="INFREQUENT"				VALUE='"infrequent"'/>
				<ITEM NAME="NONE"					VALUE='"none"'/>
				<ITEM NAME="PROJECTMISS"			VALUE='"projectMiss"'/>
				<ITEM NAME="QUERY"					VALUE='"query"'/>
				<ITEM NAME="QUERYTRACE"				VALUE='"queryTrace"'/>
				<ITEM NAME="READER"					VALUE='"reader"'/>
				<ITEM NAME="RUNTIME"				VALUE='"runtime"'/>
			</ITEMS>
		</PARAMETER>
		
		<PARAMETER NAME="EXCEPTION_THRESHOLD" FIELD="CLOSED_LIST" REQUIRED="false" NUM_ROW="30">
			<ITEMS DEFAULT="FATAL">
				<ITEM NAME="WARNING"	VALUE="WARNING"/>
				<ITEM NAME="ERROR"		VALUE="ERROR"/>
				<ITEM NAME="FATAL"		VALUE="FATAL"/>
			</ITEMS>
		</PARAMETER>
	</PARAMETERS>

	<ADVANCED_PARAMETERS>
		<PARAMETER NAME="ADVARGUMENTS" FIELD="TABLE" NUM_ROW="10" NB_LINES="5">
			<ITEMS>
				<ITEM NAME="NAME" FIELD="TEXT">
				</ITEM>
				<ITEM NAME="VALUE" />
			</ITEMS>
		</PARAMETER>
	</ADVANCED_PARAMETERS>

	<CODEGENERATION>
		<IMPORTS>
			<IMPORT NAME="odt-camel" MODULE="odt-camel-3.3.0.jar"
				UrlPath="platform:/plugin/org.talend.libraries.esb/lib/odt-camel-3.3.0.jar"
				REQUIRED="true" />
		</IMPORTS>
	</CODEGENERATION>

	<RETURNS />
</COMPONENT>
