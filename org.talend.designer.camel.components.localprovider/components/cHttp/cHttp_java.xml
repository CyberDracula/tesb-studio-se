<COMPONENT>

	<HEADER PLATEFORM="ALL" SERIAL="" VERSION="0.102" STATUS="ALPHA"

		COMPATIBILITY="ALL" AUTHOR="Talend" RELEASE_DATE="20100401A"
		STARTABLE="false">
		<SIGNATURE />
	</HEADER>

	<FAMILIES>
		<FAMILY>Messaging</FAMILY>
	</FAMILIES>

	<DOCUMENTATION>
		<URL />
	</DOCUMENTATION>

	<CONNECTORS>
		<CONNECTOR CTYPE="ROUTE" MIN_INPUT="0" MAX_INPUT="1"
			MAX_OUTPUT="1" MIN_OUTPUT="0" />
		<CONNECTOR CTYPE="ROUTE_WHEN" MAX_INPUT="1" MAX_OUTPUT="0" />
		<CONNECTOR CTYPE="ROUTE_OTHER" MAX_INPUT="1" MAX_OUTPUT="0" />
		<CONNECTOR CTYPE="ROUTE_TRY" MAX_INPUT="1" MAX_OUTPUT="0" />
		<CONNECTOR CTYPE="ROUTE_CATCH" MAX_INPUT="1" MAX_OUTPUT="0" />
		<CONNECTOR CTYPE="ROUTE_FINALLY" MAX_INPUT="1" MAX_OUTPUT="0" />
		<CONNECTOR CTYPE="ROUTE_ENDBLOCK" MAX_INPUT="1" MAX_OUTPUT="0" />
	</CONNECTORS>

	<PARAMETERS>
		<PARAMETER NAME="URI" FIELD="TEXT" NUM_ROW="4">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="METHOD" FIELD="CLOSED_LIST" NUM_ROW="5">
			<ITEMS DEFAULT="GET">
				<ITEM NAME="GET" VALUE="GET" />
				<ITEM NAME="POST" VALUE="POST" />
				<ITEM NAME="PUT" VALUE="PUT" />
				<ITEM NAME="DELETE" VALUE="DELETE" />
				<ITEM NAME="HEAD" VALUE="HEAD" />
				<ITEM NAME="OPTIONS" VALUE="OPTIONS" />
				<ITEM NAME="TRACE" VALUE="TRACE" />
			</ITEMS>
		</PARAMETER>
		
		<PARAMETER NAME="PLAIN_TEXT" FIELD="RADIO" NUM_ROW="6" SHOW_IF="(METHOD=='POST') OR (METHOD=='PUT')">
			<DEFAULT>true</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="FORM_CONTENT" FIELD="RADIO" NUM_ROW="6" SHOW_IF="(METHOD=='POST') OR (METHOD=='PUT')">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="NO_CONTENT" FIELD="RADIO" NUM_ROW="6" SHOW_IF="(METHOD=='POST') OR (METHOD=='PUT')">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="CONTENT_TYPE" FIELD="CLOSED_LIST" NUM_ROW="7" SHOW_IF="(((NO_CONTENT=='true') OR (PLAIN_TEXT=='true')) AND ((METHOD=='POST') OR (METHOD=='PUT')))">
			<ITEMS DEFAULT="PLAIN">
				<ITEM NAME="PLAIN" VALUE="text/plain" />
				<ITEM NAME="HTML" VALUE="text/html" />
				<ITEM NAME="XML" VALUE="text/xml" />
				<ITEM NAME="APP_FORM" VALUE="application/x-www-form-urlencoded" />
				<ITEM NAME="APP_XML" VALUE="application/xml" />
				<ITEM NAME="APP_JSON" VALUE="application/json" />
				<ITEM NAME="OTHER" VALUE="other" />
			</ITEMS>
		</PARAMETER>	
		
		<PARAMETER NAME="MANUAL_CONTENT_TYPE" FIELD="TEXT" NUM_ROW="7" SHOW_IF="(((METHOD=='POST') OR (METHOD=='PUT')) AND ((NO_CONTENT=='true') OR (PLAIN_TEXT =='true')) AND (CONTENT_TYPE.ITEM == 'OTHER'))">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="PARAMETERS" FIELD="TABLE" NUM_ROW="8"
			NB_LINES="8"  SHOW_IF="(METHOD=='DELETE') OR (METHOD=='GET') OR (METHOD=='HEAD') OR (((METHOD=='POST') OR (METHOD=='PUT')) AND (FORM_CONTENT=='true'))">
			<ITEMS>
				<ITEM NAME="KEY" FIELD="TEXT" />
				<ITEM NAME="VALUE" FIELD="TEXT" />
			</ITEMS>
		</PARAMETER>
		
<!-- 		<PARAMETER NAME="USE_URL_ENCODER" FIELD="CHECK" NUM_ROW="8"  SHOW_IF="(METHOD=='GET') OR (METHOD=='HEAD') OR (((METHOD=='POST') OR (METHOD=='PUT')) AND (FORM_CONTENT=='true'))">
			<DEFAULT>false</DEFAULT>
		</PARAMETER> -->

		<PARAMETER NAME="ENCODER_CHARSET" FIELD="TEXT" NUM_ROW="9" SHOW_IF="(METHOD=='DELETE') OR (METHOD=='GET') OR (METHOD=='HEAD') OR (((METHOD=='POST') OR (METHOD=='PUT')) AND (FORM_CONTENT=='true'))">
			<DEFAULT>UTF-8</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="PLAIN_TEXT_CONTENT" FIELD="MEMO" NUM_ROW="10"  SHOW_IF="((METHOD=='POST') OR (METHOD=='PUT')) AND (PLAIN_TEXT=='true')) ">
			<DEFAULT></DEFAULT>
		</PARAMETER>
		
	</PARAMETERS>

	<ADVANCED_PARAMETERS>
		<PARAMETER NAME="HEADERS" FIELD="TABLE" NUM_ROW="10"
			NB_LINES="8">
			<ITEMS>
				<ITEM NAME="KEY" FIELD="TEXT" />
				<ITEM NAME="VALUE" FIELD="TEXT" />
			</ITEMS>
		</PARAMETER>
	</ADVANCED_PARAMETERS>

	<CODEGENERATION>
		<IMPORTS>
			<IMPORT NAME="camel-spring" MODULE="camel-spring-2.9.2.jar"
				REQUIRED="true" />
			<IMPORT NAME="org.osgi.core" MODULE="org.osgi.core-4.2.0.jar"
				REQUIRED="true" />
			<IMPORT NAME="camel-osgi" MODULE="camel-osgi-2.3.0.jar"
				REQUIRED="true" />
			<IMPORT NAME="apache-camel" MODULE="camel-core-2.9.2.jar"
				REQUIRED="true" />
			<IMPORT NAME="spring-core" MODULE="spring-core-3.0.6.RELEASE.jar"
				REQUIRED="true" />
			<IMPORT NAME="talend-camel-http" MODULE="camel-http-alldep-2.9.2.jar"
				REQUIRED="true" />
			<IMPORT NAME="spring-aop" MODULE="spring-aop-3.0.6.RELEASE.jar"
				REQUIRED="true" />
			<IMPORT NAME="spring-beans" MODULE="spring-beans-3.0.6.RELEASE.jar"
				REQUIRED="true" />
			<IMPORT NAME="spring-context" MODULE="spring-context-3.0.6.RELEASE.jar"
				REQUIRED="true" />
			<IMPORT NAME="spring-context-support" MODULE="spring-context-support-3.0.6.RELEASE.jar"
				REQUIRED="true" />
			<IMPORT NAME="commons-management" MODULE="commons-management-1.0.jar"
				REQUIRED="true" />
			<IMPORT NAME="commons-logging" MODULE="commons-logging-1.1.1.jar"
				REQUIRED="true" />
			<IMPORT NAME="log4j" MODULE="log4j-1.2.16.jar" REQUIRED="true" />
			<IMPORT NAME="slf4j" MODULE="slf4j-api-1.6.1.jar" REQUIRED="true" />
			<IMPORT NAME="slf4j-log4j" MODULE="slf4j-log4j12-1.6.1.jar"
				REQUIRED="true" />
			<IMPORT NAME="jsch" MODULE="jsch.jar" REQUIRED="true" />
		</IMPORTS>
	</CODEGENERATION>

	<RETURNS />
</COMPONENT>
