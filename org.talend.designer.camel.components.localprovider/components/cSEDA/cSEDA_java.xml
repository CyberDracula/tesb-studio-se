			<IMPORT NAME="slf4j-log4j12" MODULE="slf4j-log4j12-1.7.5.jar"
				UrlPath="platform:/plugin/org.talend.libraries.esb/lib/slf4j-log4j12-1.7.5.jar"
				REQUIRED="true" />
			<IMPORT NAME="commons-logging" MODULE="commons-logging-1.1.1.jar"
				UrlPath="platform:/base/plugins/org.apache.commons.logging_1.1.1.v201101211721.jar"
				REQUIRED="true" />
