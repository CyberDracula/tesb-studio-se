<COMPONENT>

	<HEADER PLATEFORM="ALL" SERIAL="" VERSION="0.102" STATUS="ALPHA"

		COMPATIBILITY="ALL" AUTHOR="Talend" RELEASE_DATE="20100401A"
		STARTABLE="false">
		<SIGNATURE />
	</HEADER>

	<FAMILIES>
		<FAMILY>Routing</FAMILY>
	</FAMILIES>

	<DOCUMENTATION>
		<URL />
	</DOCUMENTATION>

	<CONNECTORS>
		<CONNECTOR CTYPE="ROUTE" MIN_INPUT="0" MAX_INPUT="1"
			MAX_OUTPUT="1" MIN_OUTPUT="0" />
		<CONNECTOR CTYPE="ROUTE_WHEN" MAX_INPUT="1" MAX_OUTPUT="0" />
		<CONNECTOR CTYPE="ROUTE_OTHER" MAX_INPUT="1" MAX_OUTPUT="0" />
		<CONNECTOR CTYPE="ROUTE_TRY" MAX_INPUT="1" MAX_OUTPUT="0" />
		<CONNECTOR CTYPE="ROUTE_CATCH" MAX_INPUT="1" MAX_OUTPUT="0" />
		<CONNECTOR CTYPE="ROUTE_FINALLY" MAX_INPUT="1" MAX_OUTPUT="0" />
		<CONNECTOR CTYPE="ROUTE_ENDBLOCK" MAX_INPUT="1" MAX_OUTPUT="0" />
	</CONNECTORS>

	<PARAMETERS>
		<PARAMETER NAME="LEVEL_TYPE" FIELD="CLOSED_LIST" NUM_ROW="1">
			<ITEMS DEFAULT="INFO">
				<ITEM NAME="FATAL" VALUE="FATAL" />
				<ITEM NAME="ERROR" VALUE="ERROR" />
				<ITEM NAME="WARN" VALUE="WARN" />
				<ITEM NAME="DEBUG" VALUE="DEBUG" />
				<ITEM NAME="INFO" VALUE="INFO" />
				<ITEM NAME="TRACE" VALUE="TRACE" />
				<ITEM NAME="OFF" VALUE="OFF" />
			</ITEMS>
		</PARAMETER>

		<PARAMETER NAME="USE_JDSL" FIELD="CHECK" NUM_ROW="2">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="MESSAGE" FIELD="TEXT" NUM_ROW="3" SHOW_IF="USE_JDSL=='true'">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="GROUP_SIZE" FIELD="RADIO" NUM_ROW="3"
			GROUP="OPTIONS" SHOW_IF="USE_JDSL=='false'">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="SIZE" FIELD="TEXT" NUM_ROW="4" GROUP="OPTIONS"
			SHOW_IF="(USE_JDSL=='false') AND (GROUP_SIZE=='true')">
			<DEFAULT>0</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="GROUP_INTERVAL" FIELD="RADIO" NUM_ROW="5"
			GROUP="OPTIONS" SHOW_IF="USE_JDSL=='false'">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="INTERVAL" FIELD="TEXT" NUM_ROW="6" GROUP="OPTIONS"
			SHOW_IF="(USE_JDSL=='false') AND (GROUP_INTERVAL=='true')">
			<DEFAULT>0</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="DELAY" FIELD="TEXT" NUM_ROW="7" GROUP="OPTIONS"
			SHOW_IF="(USE_JDSL=='false') AND (GROUP_INTERVAL=='true')">
			<DEFAULT>0</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="ACTIVE_ONLY" FIELD="CHECK" NUM_ROW="8"
			GROUP="OPTIONS" SHOW_IF="(USE_JDSL=='false') AND (GROUP_INTERVAL=='true')">
			<DEFAULT>true</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="FORMAT" FIELD="RADIO" NUM_ROW="9" GROUP="OPTIONS"
			SHOW_IF="USE_JDSL=='false'">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="ARGUMENTS" FIELD="TABLE" NUM_ROW="10"
			NB_LINES="8" GROUP="OPTIONS" SHOW_IF="(USE_JDSL=='false') AND (FORMAT=='true')">
			<ITEMS>
				<ITEM NAME="NAME" FIELD="TEXT">
				</ITEM>
				<ITEM NAME="VALUE" />
			</ITEMS>
		</PARAMETER>
	</PARAMETERS>

	<ADVANCED_PARAMETERS>
	</ADVANCED_PARAMETERS>

	<CODEGENERATION>
	</CODEGENERATION>

	<RETURNS />
</COMPONENT>
