<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension-point id="org.talend.designer.camel.dependencies.importPackage" name="Camel Import Package" schema="schema/org.talend.designer.camel.dependencies.importPackage.exsd"/>
   <extension-point id="org.talend.designer.camel.dependencies.bundleClasspath" name="Camel Bundle Classpath" schema="schema/org.talend.designer.camel.dependencies.bundleClasspath.exsd"/>
   <extension-point id="org.talend.designer.camel.dependencies.requireBundle" name="Require Bundle" schema="schema/org.talend.designer.camel.dependencies.requireBundle.exsd"/>
   <extension
         point="org.talend.designer.camel.dependencies.importPackage">
      <component
            componentName="cCXF">
         <importPackage
               packageName="org.apache.camel.component.cxf">
         </importPackage>
         <importPackage
               packageName="org.apache.cxf.feature">
         </importPackage>
         <importPackage
               maxVersion="6.0.0"
               minVersion="2.0.0"
               packageName="org.talend.esb.servicelocator.cxf">
         </importPackage>
         <importPackage
               maxVersion="6.0.0"
               minVersion="2.0.0"
               packageName="org.talend.esb.sam.agent.feature">
         </importPackage>
      </component>
      <component
            componentName="cHttp">
         <importPackage
               packageName="org.apache.camel.component.http">
         </importPackage>
         <importPackage
               packageName="org.apache.commons.httpclient.protocol">
         </importPackage>
         <importPackage
               packageName="org.apache.commons.httpclient.params">
         </importPackage>
      </component>
      <component
            componentName="cJMSConnectionFactory">
         <importPackage
               packageName="org.apache.camel.component.jms">
         </importPackage>
         <importPackage
               packageName="javax.jms">
         </importPackage>
         <importPackage
               packageName="org.apache.activemq">
            <predicate
                  attributeName="MQ_TYPE"
                  attributeValue="ActiveMQ">
            </predicate>
         </importPackage>
         <importPackage
               packageName="org.apache.activemq.pool">
            <predicate
                  attributeName="AMQ_POOL"
                  attributeValue="true">
            </predicate>
            <predicate
                  attributeName="MQ_TYPE"
                  attributeValue="ActiveMQ">
            </predicate>
         </importPackage>
         <importPackage
               packageName="org.apache.commons.pool">
            <predicate
                  attributeName="AMQ_POOL"
                  attributeValue="true">
            </predicate>
            <predicate
                  attributeName="MQ_TYPE"
                  attributeValue="ActiveMQ">
            </predicate>
         </importPackage>
         <importPackage
               packageName="javax.transaction">
            <predicate
                  attributeName="MQ_TYPE"
                  attributeValue="WebSphere MQ">
            </predicate>
         </importPackage>
         <importPackage
               packageName="org.springframework.jms.connection">
            <predicate
                  attributeName="WMQ_AUTH"
                  attributeValue="true">
            </predicate>
            <predicate
                  attributeName="MQ_TYPE"
                  attributeValue="WebSphere MQ">
            </predicate>
         </importPackage>
      </component>
      <importPackage
            packageName="javax.xml.bind">
      </importPackage>
      <importPackage
            packageName="javax.xml.bind.annotation">
      </importPackage>
      <importPackage
            packageName="javax.xml.namespace">
      </importPackage>
      <importPackage
            optional="true"
            packageName="javax.xml.ws">
      </importPackage>
      <importPackage
            optional="true"
            packageName="javax.jws">
      </importPackage>
      <importPackage
            optional="true"
            packageName="javax.jws.soap">
      </importPackage>
      <importPackage
            optional="true"
            packageName="org.slf4j">
      </importPackage>
      <importPackage
            optional="true"
            packageName="org.apache.commons.logging">
      </importPackage>
      <importPackage
            maxVersion="3"
            minVersion="2.7"
            packageName="org.apache.camel">
      </importPackage>
      <importPackage
            maxVersion="3"
            minVersion="2.7"
            packageName="org.apache.camel.builder">
      </importPackage>
      <importPackage
            maxVersion="3"
            minVersion="2.7"
            packageName="org.apache.camel.impl">
      </importPackage>
      <importPackage
            maxVersion="3"
            minVersion="2.7"
            packageName="org.apache.camel.management">
      </importPackage>
      <importPackage
            maxVersion="3"
            minVersion="2.7"
            packageName="org.apache.camel.model">
      </importPackage>
      <importPackage
            maxVersion="3"
            minVersion="2.7"
            packageName="org.apache.camel.osgi">
      </importPackage>
      <importPackage
            maxVersion="3"
            minVersion="2.7"
            packageName="org.apache.camel.spi">
      </importPackage>
      <importPackage
            maxVersion="3"
            minVersion="2.7"
            packageName="org.apache.camel.view">
      </importPackage>
      <importPackage
            maxVersion="2"
            minVersion="1.5"
            packageName="org.osgi.framework">
      </importPackage>
      <importPackage
            maxVersion="2.0.0"
            minVersion="1.0.0"
            packageName="org.osgi.service.blueprint">
      </importPackage>
      <importPackage
            packageName="routines.system.api">
      </importPackage>
   </extension>
   <extension
         point="org.talend.designer.camel.dependencies.bundleClasspath">
      <classpath
            attributeName="DRIVER_JAR"
            componentName="cConfig">
      </classpath>
      <classpath
            attributeName="WMQ_DRIVER_JAR"
            componentName="cJMSConnectionFactory">
         <predicate
               attributeName="MQ_TYPE"
               attributeValue="WebSphere MQ">
         </predicate>
      </classpath>
      <classpath
            attributeName="OTHER_DRIVER_JAR"
            componentName="cJMSConnectionFactory">
         <predicate
               attributeName="MQ_TYPE"
               attributeValue="Other">
         </predicate>
      </classpath>
      <classpath
            attributeName="LIBRARY"
            componentName="cTalendJob">
         <predicate
               attributeName="FROM_EXTERNAL_JAR"
               attributeValue="true">
         </predicate>
      </classpath>
   </extension>
   <extension
         point="org.talend.designer.camel.dependencies.requireBundle">
      <requireBundle
            bundleName="org.apache.camel.camel-core">
      </requireBundle>
      <component
            componentName="componentName">
         <requireBundle
               bundleName="bundleName">
         </requireBundle>
      </component>
   </extension>
   <extension
         point="org.talend.core.runtime.service">
      <Service
            class="org.talend.designer.camel.dependencies.core.util.RouterOsgiDependenciesResolver"
            serviceId="IOsgiDependenciesService">
      </Service>
   </extension>

</plugin>
