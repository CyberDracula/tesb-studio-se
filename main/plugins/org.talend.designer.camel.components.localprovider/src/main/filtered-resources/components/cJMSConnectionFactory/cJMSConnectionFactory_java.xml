                    REQUIRED="true" />
            <IMPORT NAME="org.eclipse.osgi"
                    MODULE="org.eclipse.osgi.jar"
                    UrlPath="platform:/base/plugins/org.eclipse.osgi_3.6.1.R36x_v20100806.jar"
