            <!-- camel-jms -->
            <IMPORT NAME="camel-jms"
                    MODULE="camel-jms-${camel.version}.jar"
                    UrlPath="platform:/plugin/org.talend.designer.camel.components.localprovider/lib/camel-jms-${camel.version}.jar"
                    REQUIRED="true" />
            <IMPORT NAME="spring-jms"
