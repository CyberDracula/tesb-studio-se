<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.navigator.navigatorContent">
      <navigatorContent
            activeByDefault="true"
            appearsBefore="org.talend.repository.viewer.root.contexts"
            contentProvider="org.talend.repository.view.esb.viewer.content.RoutesContentProvider"
            icon="icons/routes.png"
            id="org.talend.repository.viewer.esb.routes"
            labelProvider="org.talend.repository.viewer.label.RepositoryViewLabelProvider"
            name="%NavigatorContent.routes"
            priority="high">
         <triggerPoints>
            <or>
               <adapt
                     type="org.talend.repository.model.ProjectRepositoryNode">
               </adapt>
               <instanceof
                     value="org.talend.repository.model.ProjectRepositoryNode">
               </instanceof>
               <and>
                  <instanceof
                        value="org.talend.repository.model.RepositoryNode">
                  </instanceof>
                  <test
                        forcePluginActivation="true"
                        property="org.talend.repository.node.isRoutesTopNode">
                  </test>
               </and>
            </or>
         </triggerPoints>
         <possibleChildren>
            <and>
               <instanceof
                     value="org.talend.repository.model.RepositoryNode">
               </instanceof>
               <test
                     forcePluginActivation="true"
                     property="org.talend.repository.node.isRoutesTopNode">
               </test>
            </and>
         </possibleChildren>
         <actionProvider
               class="org.talend.repository.view.esb.viewer.action.ESBRepositoryNodeActionProvider"
               id="org.talend.repository.routes.action.provider.id">
            <enablement>
               <and>
                  <instanceof
                        value="org.talend.repository.model.RepositoryNode">
                  </instanceof>
                  <test
                        forcePluginActivation="true"
                        property="org.talend.repository.node.isRoutesTopNode">
                  </test>
                  <not>
                     <test
                           forcePluginActivation="true"
                           property="org.talend.repository.node.isDeleted">
                     </test>
                  </not>
               </and>
            </enablement>
         </actionProvider>
      </navigatorContent>
      <navigatorContent
            activeByDefault="true"
            contentProvider="org.talend.repository.view.esb.viewer.content.BeansContentProvider"
            icon="icons/beans.gif"
            id="org.talend.repository.viewer.esb.beans"
            labelProvider="org.talend.repository.viewer.label.RepositoryViewLabelProvider"
            name="%NavigatorContent.beans"
            priority="normal">
         <triggerPoints>
            <and>
               <instanceof
                     value="org.talend.repository.model.RepositoryNode">
               </instanceof>
               <or>
                  <test
                        forcePluginActivation="true"
                        property="org.talend.repository.node.code.isBeans">
                  </test>
                  <test
                        forcePluginActivation="true"
                        property="org.talend.repository.node.isCodeTopNode">
                  </test>
               </or>
            </and>
         </triggerPoints>
         <possibleChildren>
            <and>
               <instanceof
                     value="org.talend.repository.model.RepositoryNode">
               </instanceof>
               <test
                     forcePluginActivation="true"
                     property="org.talend.repository.node.code.isBeans">
               </test>
            </and>
         </possibleChildren>
         <actionProvider
               class="org.talend.repository.view.esb.viewer.action.ESBRepositoryNodeActionProvider"
               id="org.talend.repository.beans.action.provider.id">
            <enablement>
               <and>
                  <instanceof
                        value="org.talend.repository.model.RepositoryNode">
                  </instanceof>
                  <test
                        forcePluginActivation="true"
                        property="org.talend.repository.node.code.isBeans">
                  </test>
                  <not>
                     <test
                           forcePluginActivation="true"
                           property="org.talend.repository.node.isDeleted">
                     </test>
                  </not>
               </and>
            </enablement>
         </actionProvider>
      </navigatorContent>
      <navigatorContent
            activeByDefault="true"
            appearsBefore="org.talend.repository.viewer.root.contexts"
            contentProvider="org.talend.repository.view.esb.viewer.content.RouteResourceContentProvider"
            icon="icons/resource.gif"
            id="org.talend.repository.viewer.esb.route_resource"
            labelProvider="org.talend.repository.viewer.label.RepositoryViewLabelProvider"
            name="Route Resources"
            priority="normal">
         <triggerPoints>
            <or>
               <adapt
                     type="org.talend.repository.model.ProjectRepositoryNode">
               </adapt>
               <instanceof
                     value="org.talend.repository.model.ProjectRepositoryNode">
               </instanceof>
               <and>
                  <instanceof
                        value="org.talend.repository.model.RepositoryNode">
                  </instanceof>
                  <test
                        forcePluginActivation="true"
                        property="org.talend.repository.node.isRouteResourceNode">
                  </test>
               </and>
            </or>
         </triggerPoints>
         <possibleChildren>
            <and>
               <instanceof
                     value="org.talend.repository.model.RepositoryNode">
               </instanceof>
               <test
                     forcePluginActivation="true"
                     property="org.talend.repository.node.isRouteResourceNode">
               </test>
            </and>
         </possibleChildren>
         <actionProvider
               class="org.talend.repository.view.esb.viewer.action.ESBRepositoryNodeActionProvider"
               id="org.talend.repository.route.resources.action.provider.id">
            <enablement>
               <and>
                  <instanceof
                        value="org.talend.repository.model.RepositoryNode">
                  </instanceof>
                  <test
                        forcePluginActivation="true"
                        property="org.talend.repository.node.isRouteResourceNode">
                  </test>
                  <not>
                     <test
                           forcePluginActivation="true"
                           property="org.talend.repository.node.isDeleted">
                     </test>
                  </not>
               </and>
            </enablement>
         </actionProvider>
      </navigatorContent>
      <navigatorContent
            contentProvider="org.talend.repository.view.esb.viewer.content.RouteDocContentProvider"
            icon="icons/resource.gif"
            id="org.talend.repository.viewer.esb.route_doc"
            labelProvider="org.talend.repository.viewer.label.RepositoryViewLabelProvider"
            name="Route Docs"
            priority="normal">
         <triggerPoints>
            <and>
               <instanceof
                     value="org.talend.repository.model.RepositoryNode">
               </instanceof>
               <test
                     forcePluginActivation="true"
                     property="org.talend.repository.node.route.doc.isRouteDoc">
               </test>
            </and>
         </triggerPoints>
         <actionProvider
               class="org.talend.repository.view.esb.viewer.action.ESBRepositoryNodeActionProvider"
               id="org.talend.repository.route.doc.action.provider.id">
            <enablement>
               <and>
                  <instanceof
                        value="org.talend.repository.model.RepositoryNode">
                  </instanceof>
                  <test
                        property="org.talend.repository.node.route.doc.isRouteDoc">
                  </test>
                  <not>
                     <test
                           forcePluginActivation="true"
                           property="org.talend.repository.node.isDeleted">
                     </test>
                  </not>
               </and>
            </enablement>
         </actionProvider>
         <possibleChildren>
            <and>
               <instanceof
                     value="org.talend.repository.model.RepositoryNode">
               </instanceof>
               <test
                     property="org.talend.repository.node.route.doc.isRouteDoc">
               </test>
            </and>
         </possibleChildren>
      </navigatorContent>
     
   </extension>
   <extension
         point="org.eclipse.core.expressions.propertyTesters">
      <propertyTester
            class="org.talend.repository.view.esb.viewer.tester.RoutesNodeTester"
            id="org.talend.repository.node.tester.routes"
            namespace="org.talend.repository.node"
            properties="isRoutesTopNode"
            type="org.talend.repository.model.RepositoryNode">
      </propertyTester>
      <propertyTester
            class="org.talend.repository.view.esb.viewer.tester.BeansNodeTester"
            id="org.talend.repository.node.tester.code.beans"
            namespace="org.talend.repository.node.code"
            properties="isBeans"
            type="org.talend.repository.model.RepositoryNode">
      </propertyTester>
      <propertyTester
            class="org.talend.repository.view.esb.viewer.tester.RouteResourceNodeTester"
            id="org.talend.repository.node.tester.route_resource"
            namespace="org.talend.repository.node"
            properties="isRouteResourceNode"
            type="org.talend.repository.model.RepositoryNode">
      </propertyTester>
      <propertyTester
            class="org.talend.repository.view.esb.viewer.tester.RouteDocNodeTester"
            id="org.talend.repository.node.route.doc.tester"
            namespace="org.talend.repository.node.route.doc"
            properties="isRouteDoc"
            type="org.talend.repository.model.RepositoryNode">
      </propertyTester>
   </extension>
   <extension
         point="org.eclipse.ui.navigator.viewer">
      <viewerContentBinding
            viewerId="org.talend.repository.cnf.view">
         <includes>
            <contentExtension
                  pattern="org.talend.repository.viewer.esb.*">
            </contentExtension>
         </includes>
      </viewerContentBinding>
   </extension>
   <extension
         point="org.talend.repository.viewer.perspective.filter">
      <perspectiveFilter
            actionProviderId="org.talend.repository.viewer.actionbar.menu.perspectiveFilter"
            perspectiveId="org.talend.camel.perspective">
         <includes>
            <contentExtension
                  pattern="org.talend.repository.viewer.esb.*">
            </contentExtension>
            <contentExtension
                  pattern="org.talend.repository.viewer.root.contexts">
            </contentExtension>
            <contentExtension
                  pattern="org.talend.repository.viewer.root.code">
            </contentExtension>
            <contentExtension
                  pattern="org.talend.repository.viewer.root.recycleBin.*">
            </contentExtension>
            <contentExtension
                  pattern="org.talend.repository.viewer.root.content.refproject.content">
            </contentExtension>
            <contentExtension
                  pattern="org.talend.repository.view.sorter.*">
            </contentExtension>
            <contentExtension
                  pattern="org.talend.repository.viewer.root.documentation">
            </contentExtension>
            <contentExtension
                  pattern="org.talend.repository.viewer.esb.route_doc">
            </contentExtension>
         </includes>
      </perspectiveFilter>
      <perspectiveFilter
            actionProviderId="org.talend.repository.viewer.actionbar.menu.perspectiveFilter"
            perspectiveId="org.talend.rcp.perspective">
         <excludes>
            <contentExtension
                  pattern="org.talend.repository.viewer.esb.*">
            </contentExtension>
         </excludes>
      </perspectiveFilter>
   </extension>
   <extension
         point="org.talend.core.repository.repository_node_provider">
      <extraProducts
            products="CAMEL"
            type="CODE">
      </extraProducts>
      <extraProducts
            products="CAMEL"
            type="CONTEXT">
      </extraProducts>
      <extraProducts
            products="CAMEL"
            type="DOCUMENTATION">
      </extraProducts>
      <repositoryNode
            class="org.talend.repository.view.esb.doc.RouteDocumentRepositoryNode"
            folder="documentations/generated/route_docs"
            isResouce="false"
            label="Route_Docs"
            parentNodeType="GENERATED"
            products="CAMEL"
            type="ROUTE_DOCS">
      </repositoryNode>
      <repositoryNode
            class="org.talend.repository.view.esb.doc.RouteDocumentRepositoryNode"
            folder="documentations/generated/route_docs"
            isResouce="true"
            label="Route Doc"
            products="CAMEL"
            type="ROUTE_DOC">
      </repositoryNode>
   </extension>
   <extension
         point="org.talend.core.repositoryContextualsActions">
      <Action
            class="org.talend.repository.view.esb.doc.actions.GenerateAllRoutesDocAction"
            id="org.talend.repository.view.esb.GenerateRouteDocsAction"
            level="18"
            name="Generate all routes Documentation">
      </Action>
      <Action
            class="org.talend.repository.view.esb.doc.actions.OpenRouteHTMLDocAction"
            id="org.talend.repository.view.esb.openRouteHTMLDocAction"
            level="19"
            name="Open Route Documentation">
      </Action>
      <Action
            class="org.talend.repository.view.esb.doc.actions.ExportAllRoutesDocAction"
            id="org.talend.repository.view.esb.exportAllRoutesDocAction"
            level="29"
            name="Export All Routes Documentation">
      </Action>
   </extension>

</plugin>
