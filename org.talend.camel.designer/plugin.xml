      <projecttask
            beforeLogon="false"
            breaks="5.1.3"
            class="org.talend.camel.designer.migration.DisconnectErroHandlerMigrationTask"
            description="DisconnectErroHandlerMigrationTask"
            id="org.talend.camel.designer.disconnectErroHandlerMigrationTask"
            name="DisconnectErroHandlerMigrationTask"
            version="5.2.0">
      </projecttask>
      <projecttask
            beforeLogon="false"
            breaks="5.1.1"
            class="org.talend.camel.designer.migration.SwitchCamelCXFLibraryVersionMigrationTask"
            description="SwitchCamelCXFLibraryVersionMigrationTask"
            id="org.talend.camel.designer.switchCamelCXFLibraryVersionMigrationTask"
            name="SwitchCamelCXFLibraryVersionMigrationTask"
            version="5.1.2">
      </projecttask>
      <projecttask
            beforeLogon="false"
            breaks="5.1.1"
            class="org.talend.camel.designer.migration.NewJMSComponentMigrationTask"
            description="NewJMSComponentMigrationTask"
            id="org.talend.camel.designer.NewJMSComponentMigrationTask"
            name="NewJMSComponentMigrationTask"
            version="5.1.2">
      </projecttask>
       <projecttask
             beforeLogon="false"
             breaks="5.1.1"
             class="org.talend.camel.designer.migration.AggregateStrategyMigrationTask"
             description="AggregateStrategyMigrationTask"
             id="org.talend.camel.designer.AggregateStrategyMigrationTask"
             name="AggregateStrategyMigrationTask"
             version="5.1.2">
      </projecttask>
       <projecttask
             beforeLogon="false"
             breaks="5.1.1"
             class="org.talend.camel.designer.migration.UpdateCJMSProjectMigrationTask"
             description="Update cJMS using cJMSConnectionFactory"
             id="org.talend.camel.designer.UpdateCJMSProjectTask"
             name="UpdateCJMSProjectTask"
             version="5.1.2">
       </projecttask>
             <projecttask
                   beforeLogon="false"
                   breaks="5.1.1"
                   class="org.talend.camel.designer.migration.Switch292CamelCXFLibraryVersionMigrationTask"
                   description="Switch292CamelCXFLibraryVersionMigrationTask"
                   id="org.talend.camel.designer.switch292CamelCXFLibraryVersionMigrationTask"
                   name="Switch292CamelCXFLibraryVersionMigrationTask"
                   version="5.1.2">
       </projecttask>
             <projecttask
                   beforeLogon="false"
                   breaks="5.1.1"
                   class="org.talend.camel.designer.migration.Switch292ReleaseCamelVersionTask"
                   description="Switch292ReleaseCamelCXFLibraryVersionMigrationTask"
                   id="org.talend.camel.designer.switch292ReleaseCamelVersionTask"
                   name="Switch292ReleaseCamelVersionTask"
                   version="5.1.2">
      </projecttask>
             <projecttask
                   beforeLogon="false"
                   breaks="5.1.1"
                   class="org.talend.camel.designer.migration.Switch284CamelCXFLibraryVersionMigrationTask"
                   description="Switch284CamelCXFLibraryVersionMigrationTask"
                   id="org.talend.camel.designer.switch284CamelCXFLibraryVersionMigrationTask"
                   name="Switch284CamelCXFLibraryVersionMigrationTask"
                   version="5.1.2">
             </projecttask>
             <projecttask
                   beforeLogon="false"
                   breaks="5.1.1"
                   class="org.talend.camel.designer.migration.UpdatecJMSMigrationTask"
                   description="Update cJMS to use new field"
                   id="org.talend.camel.designer.migration.UpdatecJMSMigrationTask"
                   name="UpdatecJMSMigrationTask"
                   version="5.1.2">
             </projecttask>
             <projecttask
                   beforeLogon="false"
                   breaks="5.1.1"
                   class="org.talend.camel.designer.migration.Switch293ReleaseCamelVersionTask"
                   description="Switch293ReleaseCamelCXFLibraryVersionMigrationTask"
                   id="org.talend.camel.designer.switch293ReleaseCamelVersionTask"
                   name="Switch292ReleaseCamelVersionTask"
                   version="5.1.2">
      		</projecttask>
      		<projecttask
                   beforeLogon="false"
                   breaks="5.1.2"
                   class="org.talend.camel.designer.migration.CSetHeaderSupportMultiHeadersTask"
                   description="CSetHeaderSupportMultiHeadersTask"
                   id="org.talend.camel.designer.cSetHeaderSupportMultiHeadersTask"
                   name="cSetHeaderSupportMultiHeadersTask"
                   version="5.2.0">
      		</projecttask>
      		<projecttask
                   beforeLogon="false"
                   breaks="5.1.2"
                   class="org.talend.camel.designer.migration.CBeanCTalendJobMigrationTask"
                   description="CBeanCTalendJobMigrationTask"
                   id="org.talend.camel.designer.cBeanCTalendJobMigrationTask"
                   name="cBeanCTalendJobMigrationTask"
                   version="5.2.0">
      		</projecttask>
      </extension>
        <extension
         point="org.talend.core.external_component">
      <ExternalComponent
            class="org.talend.camel.designer.component.JMSExternalComponent">
      </ExternalComponent>
   </extension>
        <extension
              point="org.talend.designer.core.generators">
           <generator
                 class="org.talend.camel.designer.generator.RouteResourceGenerator"
                 mapping="ROUTE_RESOURCE_TYPE"
                 name="Route Resource Type">
           </generator>
           <generator
                 class="org.talend.camel.designer.generator.RouteComponentGenerator"
                 mapping="ROUTE_COMPONENT_TYPE"
                 name="Route Component Type">
           </generator>
        </extension>
        <extension
              point="org.talend.designer.core.check_nodes">
           <checknodes
                 class="org.talend.camel.designer.check.CheckCXFNodesService">
           </checknodes>
           <checknodes
                 class="org.talend.camel.designer.check.CheckJMSNodesService">
           </checknodes>
           <checknodes
                 class="org.talend.camel.designer.check.CheckRouteBuilderNodeService">
           </checknodes>
        </extension>
        <extension
              point="org.eclipse.core.runtime.preferences">
           <initializer
                 class="org.talend.camel.designer.prefs.CamelPreferenceInitializer">
           </initializer>
        </extension>
        <extension
              point="org.eclipse.ui.preferencePages">
           <page
                 category="org.talend.designer.core.ui.preferences.ImportExportPreferencePage"
                 class="org.talend.camel.designer.prefs.KarafPreferencePage"
                 id="org.talend.camel.designer.prefs.KarafPreferencePage"
                 name="%preferencePage.Karaf">
           </page>
           <page
                 category="org.talend.camel.designer.prefs.KarafPreferencePage"
                 class="org.talend.camel.designer.prefs.KarafMavenScriptPreferencePage"
                 id="org.talend.camel.designer.prefs.KarafMavenScriptPreferencePage"
                 name="%preferencePage.Maven">
           </page>
        </extension>
</plugin>
